{
  "name": "gluestack-ui-boilerplate",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "DARK_MODE=media expo start --android",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "jest",
    "e2e:build": "detox build -c android.emu.debug",
    "e2e:test": "detox test -c android.emu.debug",
    "release:staging": "eas update --channel staging --message 'Release to staging'",
    "release:prod": "eas update --channel production --message 'Release to production'",
    "rollback:staging": "eas channel:edit staging --branch staging",
    "rollback:prod": "eas channel:edit production --branch production",
    "detox:build": "detox build --configuration ios.sim.debug",
    "detox:test": "detox test --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.device.debug",
    "detox:test:android": "detox test --configuration android.device.debug",
    "e2e:ios": "detox build --configuration ios.sim.debug && detox test --configuration ios.sim.debug",
    "e2e:android": "detox build --configuration android.emu.debug && detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/icon": "^0.1.27",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.22",
    "@gluestack-ui/themed": "^1.1.73",
    "@gluestack-ui/toast": "^1.0.9",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@sentry/react-native": "^6.20.0",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-fetch": "^4.1.0",
    "expo": "~53.0.20",
    "expo-application": "~6.1.5",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-linking": "^7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "^5.1.4",
    "expo-status-bar": "^2.2.3",
    "expo-updates": "^0.28.17",
    "nativewind": "^4.1.23",
    "posthog-react-native": "^4.4.0",
    "posthog-react-native-session-replay": "^1.1.2",
    "react": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-css-interop": "^0.1.22",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "^4.0.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-worklets": "^0.4.1",
    "tailwindcss": "3.4.17",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^18.1.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "detox": "^20.43.0",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "jscodeshift": "0.15.2",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.8.3"
  },
  "private": true
}
